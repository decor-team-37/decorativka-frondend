{
  "name": "decorativka-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && npm run lint-css",
    "lint-css": "stylelint './src/**/*.{scss,css}'",
    "preview": "vite preview",
    "format": "prettier --write './src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "classnames": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "styled-components": "^6.1.8",
    "vite": "^5.1.4"
  },
  "devDependencies": {
    "@mate-academy/eslint-config-react-typescript": "^1.0.15",
    "@mate-academy/stylelint-config": "^0.0.12",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "typescript": "^5.2.2"
  }
}

